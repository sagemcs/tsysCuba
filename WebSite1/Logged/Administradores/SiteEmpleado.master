                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Administrar<span class="caret"></span></a>
                                    <ul class="dropdown-menu" role="menu">

                                        <% 
                                            if (HasRightsForSpecifiedMenu("Facturas") == true)
